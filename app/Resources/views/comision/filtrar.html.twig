{% extends 'base.html.twig' %}

{% block body %}
{{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" value="Create" />
    {{ form_end(form) }}
	

	<table>
        <thead>
            <tr>
                <th>Numero</th>
                <th>Profesor</th>
                <th>year</th>
				<th>dia</th>
				<th>hora inicio</th>
				<th>hora fin</th>
            </tr>
        </thead>
		 <tbody>
    {% for comision in comisions %}
	  <tr>
        <td> {{ comision.numero }}</td>
        <td> {{ comision.profesor }} </td>
        <td> {{ comision.year }}</td>
		
		{% for horario in horarios%}
			{%if comision.idcomision==horario.comisioncomision.idcomision%}
				<td>{{horario.diadia}}<td>
				<td>{{horario.inicio|date('H:i')}}<td>
				<td>{{horario.fin|date('H:i')}}<td>
				
			{%endif %}
		{% endfor %}
       </tr>
	   
     {% endfor %}

	
	
	
	 </tbody>
	 </table>
	
    {% endblock %}