<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Comision" table="comision">
    <indexes>
      <index name="fk_Comision_Materia_idx" columns="Materia"/>
    </indexes>
    <id name="idcomision" type="integer" column="idComision">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="numero" type="integer" column="Numero" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="profesor" type="string" column="Profesor" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="cuatrimestre" type="integer" column="Cuatrimestre" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="year" type="integer" column="Year" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <many-to-one field="materia" target-entity="Materia" fetch="LAZY">
      <join-columns>
        <join-column name="Materia" referenced-column-name="idMateria"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
