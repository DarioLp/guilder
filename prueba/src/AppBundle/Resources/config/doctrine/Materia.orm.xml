<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\Materia" table="materia">
    <id name="idmateria" type="integer" column="idMateria">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="nombre" type="string" column="Nombre" length="45" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="codigo" type="string" column="Codigo" length="10" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="coordinador" type="string" column="Coordinador" length="45" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
